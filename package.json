{
  "name": "trying-react",
  "version": "0.0.2",
  "license": "ISC",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "build:force": "vite build",
    "serve": "vite preview --host",
    "lint:eslint": "yarn eslint --cache --max-warnings 0 --report-unused-disable-directives \"**/*.{js,ts,tsx,vue,html}\"",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "lint:prettier": "yarn prettier --check \"**/*.{js,json,ts,vue,html}\"",
    "lint:prettier:fix": "yarn prettier --write \"**/*.{js,json,ts,vue,html}\"",
    "lint:stylelint": "yarn stylelint --cache \"**/*.{html,css,vue}\"",
    "lint:stylelint:fix": "yarn lint:stylelint --fix",
    "lint": "yarn lint:eslint && yarn lint:prettier && yarn lint:stylelint",
    "precommit": "yarn lint && yarn test && yarn build",
    "run:build": "docker-compose --profile build up"
  },
  "dependencies": {
    "firebase": "^9.17.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "6"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/vfile-message": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.4",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-html": "7.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "7.32.2",
    "nanoid": "^4.0.1",
    "postcss": "^8.4.12",
    "postcss-html": "1.5.0",
    "prettier": "2.8.4",
    "stylelint": "15.1.0",
    "stylelint-config-html": "1.1.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-standard": "30.0.1",
    "stylelint-order": "6.0.2",
    "tailwindcss": "^3.0.23",
    "typescript": "4.9.5",
    "vite": "^4.1.2"
  }
}
