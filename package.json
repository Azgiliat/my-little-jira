{
  "name": "trying-react",
  "version": "0.0.1",
  "license": "ISC",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "build:force": "vite build",
    "serve": "vite preview --host",
    "lint:eslint": "yarn eslint --cache --max-warnings 0 --report-unused-disable-directives \"**/*.{js,ts,vue,html}\"",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "lint:prettier": "yarn prettier --check \"**/*.{js,json,ts,vue,html}\"",
    "lint:prettier:fix": "yarn prettier --write \"**/*.{js,json,ts,vue,html}\"",
    "lint:stylelint": "yarn stylelint --cache \"**/*.{html,css,vue}\"",
    "lint:stylelint:fix": "yarn lint:stylelint --fix",
    "lint": "yarn lint:eslint && yarn lint:prettier && yarn lint:stylelint",
    "precommit": "yarn lint && yarn test && yarn build"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "6"
  },
  "devDependencies": {
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "5.13.0",
    "@typescript-eslint/parser": "5.13.0",
    "@vitejs/plugin-react": "^1.0.7",
    "autoprefixer": "^10.4.4",
    "eslint": "8.10.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-react": "7.29.4",
    "postcss": "^8.4.12",
    "postcss-html": "1.3.0",
    "prettier": "2.5.1",
    "stylelint": "14.5.3",
    "stylelint-config-html": "1.0.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-standard": "25.0.0",
    "stylelint-order": "5.0.0",
    "tailwindcss": "^3.0.23",
    "typescript": "4.6.2",
    "vite": "^2.8.0"
  }
}
